{
	"name": "vite-project",
	"description": "A javascript / Typescript library to be able to read and write csv files",
	"homepage": "https://github.com/welpie21/sigma-csv",
	"readme": "README.md",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"license": "MIT",
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"import": "./dist/sigma-csv.esm.js",
			"require": "./dist/sigma-csv.umd.js"
		}
	},
	"author": {
		"name": "Beau den Heijer",
		"url": "https://github.com/welpie21"
	},
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"test": "vitest",
		"coverage": "vitest run --coverage",
		"test:ui": "vitest --ui"
	},
	"devDependencies": {
		"@types/node": "^18.11.12",
		"@vitest/ui": "^0.25.6",
		"typescript": "^4.9.3",
		"vite": "^4.0.0",
		"vite-plugin-dts": "^1.7.1",
		"vitest": "0.25.6"
	}
}